{
  "name": "opium-filter",
  "description": "The fastest way to filter an array of objects",
  "version": "1.0.2",
  "main": "dist/index.js",
  "dependencies": {
    "lodash": "4.*.*"
  },
  "devDependencies": {
    "@types/lodash": "4.14.176",
    "typescript": "4.4.4"
  },
  "scripts": {
    "init": "npm run clear && npm i && npm run build",
    "build": "rm -rf ./dist && tsc",
    "clear": "rm -fr ./dist && rm -rf node_modules && rm -f package-lock.json",
    "release": "npm run build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/opium-pro/opium-filter.git"
  },
  "keywords": [
    "filter",
    "objects",
    "includes",
    "search"
  ],
  "author": "opium.pro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/opium-pro/opium-filter/issues"
  }
}
